function Person(first, last, age, gender, interests)  {
  this.name = {
    first: first,
    last: last
  };
  this.age = age;
  this.gender = gender;
  this.interests = interests;
  this.bio = function() {
    alert(`${this.name.first} ${this.name.last} ${this.age} ${this.gender}.`);
  };
  this.greeting = function()  {
    alert(`Hello! I\'m ${this.name.first} ${this.name.last}.`);
  };
};
